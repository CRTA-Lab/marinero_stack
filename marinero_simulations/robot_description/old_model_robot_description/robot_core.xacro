<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
    
    <link name="base_link"></link>

    <joint name="base_footprint_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="base_footprint"/>
    </joint>

    <link name="base_footprint"></link>

    <joint name="chassis_joint" type="fixed">
        <origin xyz="0.0 0.0 0.2445" rpy="0 0 ${pi/2}"/>
        <parent link="base_link" />
        <child link="chassis" />
    </joint>

    <link name="chassis">
        <visual>
            <origin xyz="0.0 0.0 0.014" />
            <geometry>
                <mesh filename="package://marinero_simulations/robot_description/meshes/MARINERO_old_model.dae" scale="1.0 1.0 1.0" />
            </geometry>
        </visual>
        
        <collision>
            <origin xyz="0.0 0.0 0.08" />
            <geometry>
                <box size="0.6656 1.077 0.165"/>
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.0 -0.23 0.305"/>
            <geometry>
                    <cylinder length="0.29" radius="0.18"/>
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.0 0.28 0.195" rpy="0 0 0"/>
            <geometry>
                    <box size="0.36 0.45 0.075"/>
            </geometry>
        </collision>

        <!-- <xacro:inertial_box mass="40.254" x="0.6655" y="1.075" z="0.2">
            <origin xyz="0 0 0.1" rpy="0.0 0.0 0.0" />
        </xacro:inertial_box> -->
        
        <inertial>
            <mass value="40.254" />
            <origin xyz="0.002148 -0.100388 0.1982" />     
            <inertia ixx="4.033" ixy="-0.024" ixz="0.01"
                    iyx="-0.024" iyy="1.83" iyz="0.503"
                    izx="0.01" izy="0.503" izz="4.291" />
        </inertial>
    </link>


    
    <!-- FRONT LEFT WHEEL -->

    <joint name="fl_rotation_joint" type="fixed">
        <origin xyz="0.4 0.2125 ${wheel_radius}" rpy="0 0 0"/>
        <parent link="base_link" />
        <child link="fl_axle_link" />
        <axis xyz="0 0 -1" />
    </joint>

    <link name="fl_axle_link">
        <visual>
            <origin xyz="0 0 0.1466" rpy="0 0 ${pi/2}"/>    <!-- xyz="0 0 ${0.156697-0.01}" -->
            <geometry>
                <mesh filename="package://marinero_simulations/robot_description/meshes/Wheel_Assembly_old_model.stl" scale="1.0 1.0 1.0" />
            </geometry>
            <material name="dark" />
        </visual>

        <collision>
            <origin xyz="0.0 0.0 0.124" rpy="0 0 0"/>
            <geometry>
                    <box size="0.13 0.11 0.035"/>
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.0 0.042 0.045" rpy="0 0 0"/>
            <geometry>
                    <box size="0.13 0.025 0.13"/>
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.0 -0.042 0.045" rpy="0 0 0"/>
            <geometry>
                    <box size="0.13 0.025 0.13"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="-0.006278 0.0 0.059822" rpy="0 0.0 0.0" />
            <mass value="0.322" />     
            <inertia ixx="0.001" ixy="-3.884e-6" ixz="5.365e-5"
                    iyx="-3.884e-6" iyy="0.001" iyz="1.755e-6"
                    izx="5.365e-5" izy="1.755e-6" izz="9.621e-4" />
        </inertial>
    </link>


    <joint name="fl_wheel_joint" type="continuous">
        <origin xyz="0 0 0" rpy="-${pi/2} 0 0"/>
        <parent link="fl_axle_link" />
        <child link="fl_wheel" />
        <axis xyz="0 0 1" />
        <dynamics damping="0.1"/>
        <limit effort="50" velocity="20"/>
    </joint>

    <link name="fl_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://marinero_simulations/robot_description/meshes/Wheel_old_model.stl" scale="1.0 1.0 1.0" />
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1.469" />
            <inertia ixx="0.004" ixy="0.0" ixz="0.0"
                    iyx="0.0" iyy="0.004" iyz="0.0"
                    izx="0.0" izy="0.0" izz="0.006" />
        </inertial>
    </link>


